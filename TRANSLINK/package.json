{
  "name": "translink-earbuds-experience",
  "version": "2.0.0",
  "description": "A powerful web experience for the Translink Earbuds - Refactored Architecture",
  "type": "module",
  "main": "src/core/app.js",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:performance": "jest tests/performance",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "audit:assets": "node scripts/verify-assets.js",
    "cleanup:public": "./scripts/cleanup-public.sh",
    "restore:assets": "./scripts/restore-assets.sh"
  },
  "dependencies": {
    "gsap": "^3.12.7",
    "@unseenco/taxi": "^1.8.0",
    "TagCloud": "^2.5.0",
    "lenis": "^1.2.3",
    "stats-gl": "^3.6.0",
    "three": "^0.175.0"
  },
  "devDependencies": {
    "@types/three": "^0.175.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.0",
    "@babel/plugin-proposal-private-methods": "^7.18.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-serializer-html": "^7.1.0",
    "identity-obj-proxy": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-bundle-analyzer": "^0.7.0"
  },
  "keywords": [
    "3d",
    "webgl",
    "three.js",
    "gsap",
    "earbuds",
    "interactive",
    "translink"
  ],
  "author": "OFF+BRAND",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}